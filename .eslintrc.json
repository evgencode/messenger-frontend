{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "standard",
    "standard-react",
    "prettier",
    "prettier/standard",
    "prettier/react"
  ],
  "plugins": [
    "standard",
    "prettier"
  ],
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": [2, {"trailingComma": "es5", "singleQuote": true, "semi": false}],
    "react/no-unused-prop-types": "off"
  }
}